    "sortablejs": "^1.12.0",
    "swr": "^0.5.5",
    "tippy.js": "^6.3.1"
  },
  "scripts": {
    "build": "vite build",
    "hot": "vite --force",
    "lint": "eslint --ext .jsx --ext .js frontend/src --fix",
    "pretty": "npx prettier ./frontend/src --check",
    "pretty:fix": "npx prettier ./frontend/src --check --write",
    "clear-lint-cache": "eslint . --cache --cache-file eslint-cache/eslintcache",
    "clean": "rimraf node_modules yarn.lock package-lock.json",
    "gettext-parser": "npx react-gettext-parser --output ./locale.pot --config ./gettext-parser.config.js  './frontend/src/**/{*.js,*.jsx}'",
    "i18n:json": "npx po2json locale.pot languages/bitwpfzc-admin-script.json -f jed1.x",
    "i18n:php": "[ ! -d \"languages/\" ] && mkdir languages/;node ./pot-to-php.js locale.pot languages/generatedString.php bitwpfzc;wp i18n make-pot ./ --include=*.php --skip-js --slug=bitwpfzc --headers='{\"Last-Translator\":\"support@formsintegrations.com\",\"Language-Team\":\"support@formsintegrations.com\",\"PO-Revision-Date\":\"\",\"Report-Msgid-Bugs-To\":\"\"}'",
    "production": "composer i --no-dev && pnpm install && pnpm build && pnpm gettext-parser && pnpm i18n:php"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "**/*.js?(x)": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie 11",
      "ie 10",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "ie 10",
      "ie > 9",
      "ie 9-11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@vitejs/plugin-react": "^4.3.1",
    "@wordpress/babel-plugin-makepot": "^4.1.2",
    "@wordpress/i18n": "^3.19.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^7.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12",
    "vite": "^4.4.9"
  }
}
