    "@tippyjs/react": "^4.2.5",
    "core-js": "^3.11.1",
    "create-file-list": "^1.0.1",
    "deepmerge-alt": "^0.3.0",
    "gettext-parser": "^7.0.0",
    "lodash": "^4.17.23",
    "react": "^18.2.0",
    "react-color-gradient-picker": "^0.1.2",
    "react-content-loader": "^6.0.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-date-picker": "^8.1.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.0.1",
    "react-grid-layout": "^1.2.4",
    "react-multiple-select-dropdown-lite": "^2.0.4",
    "react-router-dom": "^7.6.0",
    "react-simple-resizer": "^2.1.0",
    "react-sortablejs": "^6.0.0",
    "react-table": "^7.7.0",
    "react-table-sticky": "^1.0.0",
    "react-tabs": "^3.2.1",
    "react-time-picker": "^4.2.1",
    "react-transition-group": "^4.4.1",
